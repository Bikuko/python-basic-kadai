      "source": [
        "class Data:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name  # 名前\n",
        "        self.age = age    # 年齢\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(f\"名前: {self.name}, 年齢: {self.age}\")\n",
        "\n",
        "person1 = Data(\"侍屋　寺子\", 40)\n",
        "person2 = Data(\"侍屋　寺男\", 50)\n",
        "\n",
        "person1.printinfo()\n",
        "person2.printinfo()\n",
        "\n"
      ]
    }
  ]
