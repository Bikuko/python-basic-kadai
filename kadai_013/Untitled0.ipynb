      "source": [
        "def calculate(price, tax_rate):\n",
        "    total_price = price * (1 + tax_rate)  # 税込み金額を計算\n",
        "    return total_price\n",
        "\n",
        "price = 1000\n",
        "\n",
        "tax_rate = 0.10\n",
        "\n",
        "total = calculate(price, tax_rate)\n",
        "\n",
        "# 結果を表示\n",
        "print(f\"商品金額: {price}円\")\n",
        "print(f\"消費税率: {tax_rate * 100}%\")\n",
        "print(f\"税込み金額: {total:.2f}円\")\n",
        "\n"
      ]
